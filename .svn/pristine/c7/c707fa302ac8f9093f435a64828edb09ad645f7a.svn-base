package kr.co.mysquare.dto;

public class LocGroup 
{
	int locGroupCode; //명소분류코드
	String locGroupName; //명소분류명
	
	public int getLocGroupCode() {
		return locGroupCode;
	}
	public void setLocGroupCode(int locGroupCode) {
		this.locGroupCode = locGroupCode;
	}
	public String getLocGroupName() {
		return locGroupName;
	}
	public void setLocGroupName(String locGroupName) {
		this.locGroupName = locGroupName;
	}
	
}//end of LocGroup class

