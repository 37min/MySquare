h1,h2 {
	font-weight: normal;
	margin: 10px 0;
	padding: 10px 0
}
h6
{
		margin: 0;
	padding: 0;
}
h1 {
	font-size: 4em;
	padding: 15px 5%;
	margin: 0 auto;
	background: #2b221b;
	line-height: 1em;
	color: #e3e1d5;
	border-bottom: 1px solid #1b140e;
	text-indent: -9000px;
	background-image: url(images/header.jpg);
	background-repeat: no-repeat
}

.container {
	padding: 10px 5%
}

ul.column {
	width: 100%;
	padding: 0;
	margin: 0px 0px;
	list-style: none
}

ul.column li {
	width: 200px;
	padding: 0px;
	margin: 0px 0px;
	display: inline;
}

.block {
	height: 170px;
	width: 480px;
	font-size: 1em;
	margin-right: 10px;
	margin-bottom: 10px;
	padding: 20px;
	background: #e3e1d5;
	-moz-border-radius: 3px;
	-khtml-border-radius: 3px;
	-webkit-border-radius: 3px;
}

.block h2 {
	font-size: 1.8em;
}

.block #titleK {
	font-size: 4.0em;
	padding: 0.5%;
}

.block #bothclear
{
	clear: both;	
}

.floatleft
{
	float: left;	
}
.floatright
{
	float: right;	
}

#id_RE
{
	font-size: 2.0em;
}
#loc_RE
{
	font-size: 2.0em;
}
#context_RE
{
	margin-top: 5px;
	width: 50%;
		font-size: 1.2em;
}

.block img {
	width: 15%;
	padding: 0.5%;
	margin: 0 0 10px 0;
	background: #fff;
	-ms-interpolation-mode: bicubic;
	display: block;
	-moz-border-radius: 3px;
	-khtml-border-radius: 3px;
	-webkit-border-radius: 3px;
	float: right;
}

