#nav{
    height:35px;
    border-bottom:1px solid #ddd;
     position:fixed;
    top:0px;
    left:0px;
    right:0px;
    background:#d1d1d2 url(../images/nav.png) repeat-x center left;
}
#nav #navLeft{
    height:25px;
    float:left;
    list-style:none;
    margin:6px 0px 0px 0px;
   
}
#nav #navLeft li{
    display:inline;
    float:left;
    margin:0px 2px;
}




#nav #navRight{
    height:25px;
    float:right;
    list-style:none;
    margin:6px 40px ;
   
}
#nav #navRight li{
    display:inline;
    float:right;
   
    margin:0px 2px ;
}

#nav #logOk{
    font-size:11px;
    font-weight:bold;
    float:right;
    padding: 2px 4px;
    color:#999;
    text-decoration: none;
    border:1px solid #ccc;
    background:transparent url(../images/overlay.png) repeat-x center left;
    height:16px;
    line-height:16px;
}


#nav a{
    font-size:11px;
    font-weight:bold;
    float:right;
    padding: 2px 4px;
    color:#999;
    text-decoration: none;
    border:1px solid #ccc;
    cursor: pointer;
    background:transparent url(../images/overlay.png) repeat-x center left;
    height:16px;
    line-height:16px;
}
#nav a:hover{
    background:#D9D9DA none;
    color: #fff;
}
#nav a.top span, #nav a.bottom span{
    float:left;
    width:16px;
    height:16px;
}
#nav a.top span{
    background:transparent url(../images/top.png) no-repeat center center;
}
#nav a.bottom span{
    background:transparent url(../images/bottom.png) no-repeat center center;
}

#nav2{
	float:right;
}

#loginBtn{
	float:right;
}
#loginBtn ul
{
	float:right;
}

#loginBtn ul li
{
	float:right;
}


#nav ul li.search{
    float:left;;;
}
#nav input[type="text"]{
    float:left;
    border:1px solid #ccc;
    margin:0px 1px 0px 0px;
    padding:2px 2px 2px 2px;
}
input.searchbutton{
    border:1px solid #ccc;
    padding:1px;
    cursor:pointer;
    width:30px;
	height:22px;
    background:#E8E9EA url(../images/search.png) no-repeat center center;
}
input.searchbutton:hover{
    background-color:#D9D9DA;
}
